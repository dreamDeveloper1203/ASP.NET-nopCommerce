@model ReturnRequestActionSearchModel

<div class="panel panel-default">
    <div class="panel-heading">
        @T("Admin.Configuration.Settings.Order.ReturnRequestActions")
    </div>
    <div class="panel-body">
        <p>
            <em>@T("Admin.Configuration.Settings.Order.ReturnRequestActions.Hint")</em>
        </p>
        @await Html.PartialAsync("Table", Model.Grid)
    </div>
    <div class="panel-footer">
        <a asp-action="ReturnRequestActionCreate" asp-controller="ReturnRequest" class="btn btn-primary">
            <i class="fa fa-plus-square"></i>
            @T("Admin.Common.AddNew")
        </a>
    </div>
</div>